  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.4",
    "babel-preset-es2015": "^6.1.2",
    "digest-stream": "^0.2.2",
    "help2md": "^1.0.6",
    "mocha": "^2.3.3",
    "standard": "^5.3.1"
  },
  "babel": {
    "presets": [
      "es2015"
